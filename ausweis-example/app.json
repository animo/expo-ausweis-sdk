{
  "expo": {
    "name": "ausweis-example",
    "slug": "ausweis-example",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "ausweis-example",
    "userInterfaceStyle": "automatic",
    "plugins": [
      "expo-router",
      "../app.plugin.js",
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26,
            "useLegacyPackaging": true
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "id.animo.ausweissdk.example"
    },
    "ios": {
      "bundleIdentifier": "id.animo.ausweissdk.example"
    }
  }
}
